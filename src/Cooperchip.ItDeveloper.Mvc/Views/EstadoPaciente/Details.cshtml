@{
    @using Cooperchip.Demo.Domain.Entities;
    @model EstadoPaciente
    ViewData["Title"] = "Detalhes de Estado do Paciente";
}

<partial name="PartialViews/_cabecalho" />

<div class="panel panel-success panel-controls">

    <div class="panel panel-heading">
        <h3>Detalhes de Estado do Paciente</h3>
    </div>

    <div class="panel panel-body">
        <dl class="dl-horizontal">
            <dt>
                @Html.DisplayNameFor(x => x.Descricao)
            </dt>
            <dd>
                @Html.DisplayFor(x => x.Descricao)
            </dd>
        </dl>
        <div>
            <a class="btn btn-primary" asp-controller="EstadoPaciente" asp-action="EditPut" asp-route-id="@Model.Id">Editar</a>
            <a class="btn btn-default" asp-controller="EstadoPaciente" asp-action="Index" class="btn btn-default">Voltar para Lista</a>
        </div>
    </div>

</div>